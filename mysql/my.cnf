
[mysqladmin]
user=root

[mysqld]
basedir=/Applications/mampstack-7.3.16-2/mysql
datadir="/Applications/mampstack-7.3.16-2/mysql/data"
port=3306
socket=/Applications/mampstack-7.3.16-2/mysql/tmp/mysql.sock
tmpdir=/Applications/mampstack-7.3.16-2/mysql/tmp
max_allowed_packet=32M
bind-address=127.0.0.1
skip-name-resolve=1
expire_logs_days=7
character-set-server=utf8mb4
collation-server=utf8mb4_general_ci

default-authentication-plugin=mysql_native_password
[client]
port=3306
socket=/Applications/mampstack-7.3.16-2/mysql/tmp/mysql.sock

default-character-set=utf8mb4
[manager]
port=3306
socket=/Applications/mampstack-7.3.16-2/mysql/tmp/mysql.sock
pid-file=/Applications/mampstack-7.3.16-2/mysql/tmp/manager.pid
default-mysqld-path=/Applications/mampstack-7.3.16-2/mysql/bin/mysqld.bin

